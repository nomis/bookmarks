<h1>Editing Bookmark</h1>
<%= render 'form', bookmark: @bookmark %>
<ul class="menu">
	<li><%= link_to "Reset", edit_bookmark_path(@bookmark), rel: "nofollow" %></li>
	<li><%= link_to "Back", bookmark_path(@bookmark), rel: "nofollow" %></li>
	<li><%= link_to "Delete", bookmark_path(@bookmark), method: :delete, data: { confirm: 'Are you sure?' } %></li>
</ul>

